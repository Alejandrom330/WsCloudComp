<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="peli2" class="demo.modelo.entidad.Pelicula" scope="prototype">
		<property name="titulo" value="peli"></property>
		<property name="anioSalida" value="1"></property>
	</bean>

	<bean id="peli3" class="demo.modelo.entidad.Pelicula" scope="prototype">
		<property name="titulo" value="Otra peli"></property>
		<property name="anioSalida" value="2"></property>
	</bean>

</beans>
